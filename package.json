    "@types/express-ws": "3.0.5",
    "@types/jsonpath": "0.2.4",
    "@types/lodash": "4.17.19",
    "@types/node": "20.14.2",
    "@types/uuid": "10.0.0",
    "cross-env": "7.0.3",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "nodemon": "3.1.3",
    "prettier": "3.0.2",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.16",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5",
    "typescript-eslint": "8.1.0"
  }
