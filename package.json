{
  "name": "openapi-ui-k8s-bff",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc --noEmit false && tsc-alias",
    "start": "node dist/index.js",
    "dev:prod": "cross-env PORT=4002 ts-node -r tsconfig-paths/register dist/index.js",
    "dev": "cross-env DEVELOPMENT=TRUE PORT=4002 nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node -r tsconfig-paths/register src/index.ts\"",
    "tsc": "tsc --project tsconfig.json",
    "lint:js": "eslint",
    "lint": "npm run lint:js && npm run tsc",
    "swagger:gen": "ts-node ./src/swagger.ts"
  },
  "dependencies": {
    "@readme/openapi-parser": "4.0.0",
    "apicache": "1.6.3",
    "axios": "1.10.0",
    "body-parser": "1.20.2",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "express-healthcheck": "0.1.0",
    "express-prom-bundle": "8.0.0",
    "express-winston": "4.2.0",
    "express-ws": "5.0.2",
    "jsonpath": "1.1.1",
    "lodash": "4.17.21",
    "node-cache": "^5.1.2",
    "openapi-types": "12.1.3",
    "prom-client": "15.1.3",
    "swagger-autogen": "2.23.7",
    "swagger-ui-express": "5.0.1",
    "uuid": "10.0.0",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
